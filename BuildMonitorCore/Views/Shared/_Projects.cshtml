@model BuildMonitorCore.Models.Home.BuildMonitorViewModel
@{
    Layout = null;
}

@foreach (var project in Model.Projects)
{
    if (project.Builds.Count == 0)
    {
        continue;
    }

    <div class="projectRow">
        <h3 class="projectTitle">@project.Name</h3>
        <div class="row">
            @foreach (var build in project.Builds)
            {
                <partial model="@build" name="~/Views/Shared/_BuildItem.cshtml"/>

            }
        </div>
    </div>
}

